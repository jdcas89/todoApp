{
    "env": {
        "node": true,
        "browser": true, 
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules":{
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-use-before-define":"off",
        "no-console":"off",
        "react/prop-types":"ignore",
        "no-undef":"off"
    }
}
